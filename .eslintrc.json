{
  "extends": [
    "./node_modules/gts/",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime"
  ],
  "rules": {
    "node/no-unpublished-import": 0,
    "node/no-unpublished-require": 0,
    "node/no-unsupported-features/es-syntax": 0
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true
  }
}
